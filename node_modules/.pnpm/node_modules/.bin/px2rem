#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/starloom/node_modules/.pnpm/px2rem@0.5.0/node_modules/px2rem/bin/node_modules:/workspace/starloom/node_modules/.pnpm/px2rem@0.5.0/node_modules/px2rem/node_modules:/workspace/starloom/node_modules/.pnpm/px2rem@0.5.0/node_modules:/workspace/starloom/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/starloom/node_modules/.pnpm/px2rem@0.5.0/node_modules/px2rem/bin/node_modules:/workspace/starloom/node_modules/.pnpm/px2rem@0.5.0/node_modules/px2rem/node_modules:/workspace/starloom/node_modules/.pnpm/px2rem@0.5.0/node_modules:/workspace/starloom/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../px2rem/bin/px2rem.js" "$@"
else
  exec node  "$basedir/../px2rem/bin/px2rem.js" "$@"
fi
