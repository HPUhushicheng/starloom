#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/starloom/node_modules/.pnpm/strip-ansi@0.3.0/node_modules/strip-ansi/node_modules:/workspace/starloom/node_modules/.pnpm/strip-ansi@0.3.0/node_modules:/workspace/starloom/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/starloom/node_modules/.pnpm/strip-ansi@0.3.0/node_modules/strip-ansi/node_modules:/workspace/starloom/node_modules/.pnpm/strip-ansi@0.3.0/node_modules:/workspace/starloom/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../strip-ansi/cli.js" "$@"
else
  exec node  "$basedir/../strip-ansi/cli.js" "$@"
fi
